    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "express": "^4.21.1",
    "html-to-text": "^9.0.5",
    "langchain": "^0.1.30",
    "pdf-parse": "^1.1.1",
    "tslib": "^2.8.1",
    "winston": "^3.13.0",
    "ws": "^8.17.1",
    "zod": "^3.22.4"
  }
}
