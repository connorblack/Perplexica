  "scripts": {
    "start": "npx concurrently \"npm run db:push && npx tsx src/app.ts\" \"npm run start --prefix ui\"",
    "build": "tsc",
    "dev": "npx concurrently \"npx tsx watch src/app.ts\" \"npm run dev --prefix ui\"",
    "db:push": "drizzle-kit push sqlite",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "install:all": "pnpm install && pnpm install --prefix ui"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^22.9.3",
    "@types/pdf-parse": "^1.1.4",
    "@types/readable-stream": "^4.0.11",
    "@types/ws": "^8.5.12",
    "drizzle-kit": "^0.22.7",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@langchain/anthropic": "^0.2.3",
    "@langchain/community": "^0.2.16",
    "@langchain/core": "^0.3.18",
    "@langchain/openai": "^0.0.25",
    "@types/express-serve-static-core": "^5.0.1",
    "@xenova/transformers": "^2.17.1",
    "axios": "^1.6.8",
